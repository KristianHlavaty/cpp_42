  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch PmergeMe",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/M09/ex02/PmergeMe",
      "args": [
        "3",
        "5",
        "9",
        "7",
        "4"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/M09/ex02",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Skip system libraries during debugging",
          "text": "set sysroot /",
          "ignoreFailures": true
        },
        {
          "description": "Don't step into system library functions",
          "text": "set auto-load safe-path /",
          "ignoreFailures": true
        }
      ]
    },
